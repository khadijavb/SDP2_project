def read_experience_lines(filename):
    experience_lines = []
    with open(filename) as f:
        for line in f:
            if line.strip() == "Experience":
                # read the next lines until an empty line is encountered
                next_line = next(f).strip()
                while next_line != "":
                    experience_lines.append(next_line)
                    next_line = next(f).strip()
                break
    return experience_lines

def read_skills_lines(filename):
    skills_lines = []
    with open(filename) as f:
        for line in f:
            if line.strip() == "Skills":
                # read the next lines until an empty line is encountered
                skl_line = next(f).strip()
                while skl_line != "":
                    skills_lines.append(skl_line)
                    skl_line = next(f).strip()
                break
    return skills_lines

def read_degree_lines(filename):
    degree_lines = []
    with open(filename) as f:
        for line in f:
            if line.strip() == "Degree":
                # read the next lines until an empty line is encountered
                dgr_line = next(f).strip()
                while dgr_line != "":
                    degree_lines.append(dgr_line)
                    dgr_line = next(f).strip()
                break
    return degree_lines

